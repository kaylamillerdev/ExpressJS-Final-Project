<form id="logout" name="logout" method="GET" action="/users/logout">
<h1>Hello, {{user.FirstName}} {{user.LastName}}</h1>
<h2>Email: {{user.Email}}</h2>
<h3>Username: {{user.Username}}</h3>
<h3>UserId: {{user.UserId}}</h3>

<button type="submit" onclick="users/logout">Logout</button>
</form>

<h3>Posts:</h3>
{{#each post}}
<h2>{{post.PostId}}</h2>
<h2>{{post.PostTitle}}</h2>
<h2>{{post.PostBody}}</h2>
{{/each}}

